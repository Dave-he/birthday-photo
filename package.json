{
  "name": "birthday-photo-monorepo",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@9.15.0",
  "scripts": {
    "dev": "pnpm -r dev",
    "build": "pnpm -r build",
    "start": "pnpm -r start",
    "test": "pnpm -r test",
    "lint": "pnpm -r lint",
    "typecheck": "pnpm -r typecheck || pnpm -r ts:check",
    "web:dev": "pnpm --filter web dev",
    "admin:dev": "pnpm --filter admin dev",
    "web:test": "pnpm --filter web test",
    "admin:test": "pnpm --filter admin test",
    "deploy": "docker-compose up -d --build"
  },
  "pnpm": {
    "overrides": {
      "react": "19.2.1",
      "react-dom": "19.2.1",
      "@types/react": "^19",
      "@types/react-dom": "^19"
    }
  },
  "devDependencies": {
    "typescript": "^5.0.0"
  }
}
